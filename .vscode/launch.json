  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Main Generator",
      "runtimeArgs": ["${workspaceFolder}/node_modules/yo/lib/cli.js"],
      "program": "${workspaceFolder}/src/generators/app/index.ts",
      "cwd": "${workspaceFolder}/debug",
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "skipFiles": ["<node_internals>/**"],
      "preLaunchTask": "build and clean-debug"
    },
    {
      "type": "node",
      "request": "launch",
