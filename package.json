{
  "name": "generator-cdspackage",
  "version": "0.1.0",
  "description": "A generator for creating the entire package for CDS (MS Dynamics and Power Apps) solutions",
  "author": {
    "name": "Capgemini",
    "email": "tom.ashworth@capgemini.com"
  },
  "files": [
    "dist/generators/"
  ],
  "keywords": [
    "",
    "yeoman-generator"
  ],
  "devDependencies": {
    "@types/node": "^10.12.18",
    "@types/yeoman-generator": "^3.0.1",
    "@types/yosay": "0.0.29",
    "eslint": "^5.9.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-config-xo": "^0.25.0",
    "eslint-plugin-prettier": "^3.0.0",
    "husky": "^1.2.0",
    "jest": "^23.5.0",
    "lint-staged": "^8.1.0",
    "prettier": "^1.15.2",
    "tslint": "^5.12.1",
    "typescript": "^3.2.4",
    "yeoman-assert": "^3.1.0",
    "yeoman-test": "^1.7.0"
  },
  "engines": {
    "npm": ">= 4.0.0"
  },
  "dependencies": {
    "@types/rimraf": "^2.0.2",
    "azure-devops-node-api": "^7.0.0",
    "chalk": "^2.1.0",
    "glob": "^7.1.3",
    "glob-promise": "^3.4.0",
    "renamer": "^1.1.1",
    "rimraf": "^2.6.3",
    "simple-git": "^1.107.0",
    "yeoman-generator": "^2.0.1",
    "yosay": "^2.0.1"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": "error"
    },
    "plugins": [
      "prettier"
    ]
  },
  "scripts": {
    "pretest": "eslint .",
    "test": "jest"
  },
  "license": "unlicense"
}
